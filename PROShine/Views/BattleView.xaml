                <WrapPanel>
                    <p:OutlinedTextBlock x:Name="ActiveType1" Margin="10,0,5,0" StrokeThickness="3" FontSize="18" TextWrapping="NoWrap" Text="[Type1]" FontWeight="SemiBold" TextAlignment="Right">
